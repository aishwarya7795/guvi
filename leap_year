Import java.util.Scanner;
public class MyClass {
    public static void main(String args[]) {
           {
               Scanner sc= new Scanner(System.in);
             int n1=sc.nextInt();
        
    
            if((n1%4==0)||(n1%400==0)){
                    System.out.println("Leap Year"+n1);
            }
           else{
                        System.out.println("not a leap year"+n1);
        }

}
}
}
